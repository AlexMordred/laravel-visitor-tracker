{
    "name": "voerro/laravel-visit-stats",
    "description": "Visit tracker and statistics for Laravel 5",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Zavyalov",
            "email": "alex@voerro.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0",
        "illuminate/support": "5.5.*",
        "illuminateh/http": "5.5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "orchestra/testbench": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Voerro\\VisitStats\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Voerro\\VisitStats\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Voerro\\VisitStats\\VisitStatsServiceProvider"
            ],
            "aliases": {
                "SimplePackage": "Voerro\\VisitStats\\Facades\\Tracker"
            }
        }
    }
}
